{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "jsx": "never",
        "json": "always"
      }
    ],
    "import/no-unresolved": "off"
  },
  "ignorePatterns": ["dist/"]
}
